<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_torpedoes">

	<executable package_path="sonia_flexbe_behaviors.vision_torpedoes_sm" class="vision_torpedoesSM" />
	<tagstring>task</tagstring>
	<author>CS</author>
	<date>Wed Jul 13 2022</date>
	<description>
		Behavior to detect the gman.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="enum" name="vision_torpedoes_filterchain" default="simulation_gman" label="vision_torpedoes_filterchain" hint="Sets the vision_torpedoes_filterchain">
			<option value="simulation_gman" />
		</param>

		<param type="enum" name="vision_torpedoes_target" default="gman" label="vision_torpedoes_target" hint="Sets the vision_torpedoes_target">
			<option value="gman" />
		</param>

		<param type="numeric" name="camera_no" default="1" label="camera_no" hint="Sets the camera_no">
			<min value="1" />
			<max value="4" />
		</param>

		<param type="numeric" name="bounding_box_width" default="0" label="bounding_box_width" hint="Sets the bounding_box_width">
			<min value="0" />
			<max value="600" />
		</param>

		<param type="numeric" name="bounding_box_height" default="0" label="bounding_box_height" hint="Sets the bounding_box_height">
			<min value="0" />
			<max value="600" />
		</param>

	</params>


</behavior>