<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_torpedoes">

	<executable package_path="sonia_flexbe_behaviors.vision_torpedoes_sm" class="vision_torpedoesSM" />
	<tagstring>task</tagstring>
	<author>CS</author>
	<date>Wed Jul 13 2022</date>
	<description>
		Behavior to detect the gman.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="enum" name="torpedoes_filterchain" default="deep_compe_front" label="torpedoes_filterchain" hint="Sets the torpedoes_filterchain">
			<option value="deep_compe_front" />
			<option value="deep_simulation_front" />
			<option value="sift_front" />
			<option value="simple_buoy" />
			<option value="simulation_torpedoes_star" />
			<option value="simple_torpedoes_star" />
		</param>

		<param type="enum" name="torpedoes_target" default="torpedoes" label="torpedoes_target" hint="Sets the torpedoes_target">
			<option value="torpedoes" />
		</param>

		<param type="numeric" name="camera_no" default="1" label="camera_no" hint="Sets the camera_no">
			<min value="1" />
			<max value="4" />
		</param>

		<param type="numeric" name="torpedoes_bounding_box_width" default="300" label="torpedoes_bounding_box_width" hint="Sets the torpedoes_bounding_box_width">
			<min value="0" />
			<max value="600" />
		</param>

		<param type="numeric" name="torpedoes_bounding_box_height" default="300" label="torpedoes_bounding_box_height" hint="Sets the torpedoes_bounding_box_height">
			<min value="0" />
			<max value="600" />
		</param>

		<param type="numeric" name="torpedoes_center_bounding_box_height" default="100" label="torpedoes_center_bounding_box_height" hint="Sets the torpedoes_center_bounding_box_height">
			<min value="0" />
			<max value="600" />
		</param>

		<param type="numeric" name="torpedoes_center_bounding_box_width" default="100" label="torpedoes_center_bounding_box_width" hint="Sets the torpedoes_center_bounding_box_width">
			<min value="0" />
			<max value="600" />
		</param>

		<param type="numeric" name="torpedoes_max_mouv" default="0.5" label="torpedoes_max_mouv" hint="Sets the torpedoes_max_mouv">
			<min value="0" />
			<max value="2" />
		</param>

		<param type="numeric" name="torpedoes_min_mouv" default="0.1" label="torpedoes_min_mouv" hint="Sets the torpedoes_min_mouv">
			<min value="0" />
			<max value="1" />
		</param>

	</params>


</behavior>