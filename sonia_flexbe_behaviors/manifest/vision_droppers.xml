<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_droppers">

	<executable package_path="sonia_flexbe_behaviors.vision_droppers_sm" class="vision_droppersSM" />
	<tagstring>pool</tagstring>
	<author>Camille Sauvain</author>
	<date>06/07/2022</date>
	<description>
		Find the bin with the right filter chain, then drop the markers in the bin.
	</description>


	<!-- Contained Behaviors -->
	<contains name="search_zigzag" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="filterchain" default="simulation_cover" label="filterchain" hint="Sets the filterchain" />

		<param type="text" name="header_name" default="obstacle" label="header_name" hint="Sets the header_name" />

		<param type="numeric" name="camera_no" default="2" label="camera_no" hint="Sets the camera_no">
			<min value="1" />
			<max value="4" />
		</param>

	</params>


</behavior>