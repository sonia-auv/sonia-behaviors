<?xml version="1.0" encoding="UTF-8"?>

<behavior name="search_bottom">

	<executable package_path="sonia_flexbe_behaviors.search_bottom_sm" class="search_bottomSM" />
	<tagstring>pool</tagstring>
	<author>FA</author>
	<date>Mon Nov 15 2021</date>
	<description>
		Looking for a vision target on bottom camera
	</description>


	<!-- Contained Behaviors -->
	<contains name="snake_mouvement" />

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="move_time" default="15" label="move_time" hint="Sets the move_time">
