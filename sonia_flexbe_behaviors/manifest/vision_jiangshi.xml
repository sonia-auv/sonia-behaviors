<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_jiangshi">

	<executable package_path="sonia_flexbe_behaviors.vision_jiangshi_sm" class="vision_jiangshiSM" />
	<tagstring>pool</tagstring>
	<author>William Brouillard</author>
	<date>Sat Nov 13 2021</date>
	<description>
		Detect the jiangshi and ram into it.
	</description>


	<!-- Contained Behaviors -->
	<contains name="search_front" />
	<contains name="Aligment with stopping" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="filtername" default="deep_jiangshi" label="filtername" hint="Sets the filtername" />

		<param type="numeric" name="cam_number" default="1" label="cam_number" hint="Sets the cam_number">
			<min value="1" />
			<max value="4" />
		</param>

		<param type="text" name="header_name" default="jiangshi" label="header_name" hint="Sets the header_name" />

	</params>


</behavior>