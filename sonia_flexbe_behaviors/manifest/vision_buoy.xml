<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_buoy">

	<executable package_path="sonia_flexbe_behaviors.vision_buoy_sm" class="vision_buoySM" />
	<tagstring>pool</tagstring>
	<author>FA</author>
	<date>Sat Nov 13 2021</date>
	<description>
		Detect the buoy to find the octogon
	</description>


	<!-- Contained Behaviors -->
	<contains name="search_zigzag" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="filter_name" default="simple_sift" label="filter_name" hint="Sets the filter_name" />

		<param type="numeric" name="camera_no" default="3" label="camera_no" hint="Sets the camera_no">
			<min value="1" />
			<max value="4" />
		</param>

		<param type="text" name="header_name" default="makeGrade_tommyGun" label="header_name" hint="Sets the header_name" />

	</params>


</behavior>