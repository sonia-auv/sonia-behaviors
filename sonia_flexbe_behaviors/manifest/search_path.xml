<?xml version="1.0" encoding="UTF-8"?>

<behavior name="search_path">

	<executable package_path="sonia_flexbe_behaviors.search_path_sm" class="search_pathSM" />
	<tagstring>Task</tagstring>
	<author>KY</author>
	<date>Wed May 11 2022</date>
	<description>
		Search for path and get movement from vision filterchain.
	</description>


	<!-- Contained Behaviors -->
	<contains name="search_zigzag" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="filterchain_name" default="simple_pipe_straight" label="filterchain_name" hint="Sets the filterchain_name" />

		<param type="text" name="header_name" default="pipe straight" label="header_name" hint="Sets the header_name" />

		<param type="numeric" name="camera_no" default="4" label="camera_no" hint="Sets the camera_no">
			<min value="0" />
			<max value="4" />
		</param>

	</params>


</behavior>