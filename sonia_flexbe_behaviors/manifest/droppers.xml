<?xml version="1.0" encoding="UTF-8"?>

<behavior name="droppers">

	<executable package_path="sonia_flexbe_behaviors.droppers_sm" class="droppersSM" />
	<tagstring>pool</tagstring>
	<author>FA</author>
	<date>Tue Nov 16 2021</date>
	<description>
		Drop the markers in the target detected by the vision. It suppose that the sub is already aligned in the middle of the target.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="depth_to_target" default="1" label="depth_to_target" hint="Sets the depth_to_target">
			<min value="0" />
			<max value="4" />
		</param>

		<param type="numeric" name="x_offset" default="-0.18" label="x_offset" hint="Sets the x_offset">
			<min value="-1" />
			<max value="1" />
		</param>

		<param type="numeric" name="y_offset" default="0.1" label="y_offset" hint="Sets the y_offset">
			<min value="-1" />
			<max value="1" />
		</param>

	</params>


</behavior>