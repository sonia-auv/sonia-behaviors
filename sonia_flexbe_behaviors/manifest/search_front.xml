<?xml version="1.0" encoding="UTF-8"?>

<behavior name="search_front">

	<executable package_path="sonia_flexbe_behaviors.search_front_sm" class="search_frontSM" />
	<tagstring>search</tagstring>
	<author>FA</author>
	<date>Sun Nov 14 2021</date>
	<description>
		Look for a vision target on the front camera
	</description>


	<!-- Contained Behaviors -->
	<contains name="snake_mouvement" />

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="move_time" default="15" label="move_time" hint="Sets the move_time">
			<min value="0" />
			<max value="60" />
		</param>

		<param type="numeric" name="time_to_stop" default="1" label="time_to_stop" hint="Sets the time_to_stop">
			<min value="0" />
			<max value="10" />
		</param>

	</params>


</behavior>