<?xml version="1.0" encoding="UTF-8"?>

<behavior name="vision_path">

	<executable package_path="sonia_flexbe_behaviors.vision_path_sm" class="vision_pathSM" />
	<tagstring>pool</tagstring>
	<author>FA</author>
	<date>Mon Nov 15 2021</date>
	<description>
		Behaviors for the task of the path and rotate to the right orientation
	</description>


	<!-- Contained Behaviors -->
	<contains name="search_bottom" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="filterchain_name" default="simple_pipe45" label="filterchain_name" hint="Sets the filterchain_name" />

		<param type="numeric" name="cam_number" default="2" label="cam_number" hint="Sets the cam_number">
			<min value="1" />
			<max value="4" />
		</param>

		<param type="text" name="header_name" default="pipe" label="header_name" hint="Sets the header_name" />

	</params>


</behavior>