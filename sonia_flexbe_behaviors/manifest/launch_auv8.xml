<?xml version="1.0" encoding="UTF-8"?>

<behavior name="launch_AUV8">

	<executable package_path="sonia_flexbe_behaviors.launch_auv8_sm" class="launch_AUV8SM" />
	<tagstring>task</tagstring>
	<author>GS</author>
	<date>27/08/2022</date>
	<description>
		Shift and launch torpedoes for AUV8
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="boolean" name="activate_launch_auv8" default="True" label="activate_launch_auv8" hint="Sets the activate_launch_auv8" />

		<param type="numeric" name="launch_x" default="0.3" label="launch_x" hint="Sets the launch_x">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="launch_y" default="0.07" label="launch_y" hint="Sets the launch_y">
			<min value="-1" />
			<max value="1" />
		</param>

		<param type="numeric" name="launch_z" default="-0.1" label="launch_z" hint="Sets the launch_z">
			<min value="-1" />
			<max value="1" />
		</param>

	</params>


</behavior>