<?xml version="1.0" encoding="UTF-8"?>

<behavior name="AUV8_SEMIFINAL_SONAR">

	<executable package_path="sonia_flexbe_behaviors.auv8_semifinal_sonar_sm" class="AUV8_SEMIFINAL_SONARSM" />
	<tagstring>compe</tagstring>
	<author>GS</author>
	<date>Sat Jul 30 2022</date>
	<description>
		Full run for AUV8 with torpedoes detected with sonar
	</description>


	<!-- Contained Behaviors -->
	<contains name="CoinFlip-Gate-Trickshot with com" />
	<contains name="move" />
	<contains name="sonar_end_2_end" />
	<contains name="sonar_table_alignement" />
	<contains name="torpedoes" />

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="angle_after_path" default="45" label="angle_after_path" hint="Sets the angle_after_path">
			<min value="-180" />
			<max value="180" />
		</param>

	</params>


</behavior>