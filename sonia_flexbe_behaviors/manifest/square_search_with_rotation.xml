<?xml version="1.0" encoding="UTF-8"?>

<behavior name="square_search_with_rotation">

	<executable package_path="sonia_flexbe_behaviors.square_search_with_rotation_sm" class="square_search_with_rotationSM" />
	<tagstring></tagstring>
	<author>Willy Kao</author>
	<date>Wed Jul 27 2022</date>
	<description>
		The sub does an incremental rotation to 360 degrees before every square search.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="boxX" default="1.0" label="boxX" hint="Sets the boxX">
			<min value="0" />
			<max value="4" />
		</param>

		<param type="numeric" name="boxY" default="1.0" label="boxY" hint="Sets the boxY">
			<min value="0" />
			<max value="4" />
		</param>

		<param type="numeric" name="speed" default="2" label="speed" hint="Sets the speed">
			<min value="0" />
			<max value="2" />
		</param>

	</params>


</behavior>