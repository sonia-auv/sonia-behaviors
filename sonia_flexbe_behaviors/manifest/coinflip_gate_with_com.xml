<?xml version="1.0" encoding="UTF-8"?>

<behavior name="CoinFlip-Gate with com">

	<executable package_path="sonia_flexbe_behaviors.coinflip_gate_with_com_sm" class="CoinFlipGatewithcomSM" />
	<tagstring>task</tagstring>
	<author>FA</author>
	<date>Sun Jul 17 2022</date>
	<description>
		Orient to gate for coin flip task and starts to go to the gate with the state for the underwater communication.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="orientation_to_gate" default="0" label="orientation_to_gate" hint="Sets the orientation_to_gate">
			<min value="0" />
			<max value="360" />
		</param>

		<param type="numeric" name="dive_depth" default="1" label="dive_depth" hint="Sets the dive_depth">
			<min value="0" />
			<max value="5" />
		</param>

		<param type="numeric" name="distance_to_gate" default="4" label="distance_to_gate" hint="Sets the distance_to_gate">
			<min value="0" />
			<max value="10" />
		</param>

	</params>


</behavior>