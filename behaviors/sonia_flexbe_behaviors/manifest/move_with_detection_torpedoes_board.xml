<?xml version="1.0" encoding="UTF-8"?>

<behavior name="move_with_detection_torpedoes_board">

	<executable package_path="sonia_flexbe_behaviors.move_with_detection_torpedoes_board_sm" class="move_with_detection_torpedoes_boardSM" />
	<tagstring>move</tagstring>
	<author>GS</author>
	<date>Tue Aug 02 2022</date>
	<description>
		move_with_detection_torpedoes_board
	</description>


	<!-- Contained Behaviors -->
	<contains name="move" />

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="move_for_tb" default="15.0" label="move_for_tb" hint="Sets the move_for_tb">
			<min value="-30" />
			<max value="30" />
		</param>

		<param type="enum" name="move_with_detection_torpedoes_board_filterchain" default="deep_compe_front" label="move_with_detection_torpedoes_board_filterchain" hint="Sets the move_with_detection_torpedoes_board_filterchain">
			<option value="deep_compe_front" />
			<option value="deep_simulation_front" />
		</param>

		<param type="enum" name="move_with_detection_torpedoes_board_target" default="G-Man" label="move_with_detection_torpedoes_board_target" hint="Sets the move_with_detection_torpedoes_board_target">
			<option value="G-Man" />
		</param>

		<param type="numeric" name="turn_for_tb" default="0" label="turn_for_tb" hint="Sets the turn_for_tb">
			<min value="-180" />
			<max value="180" />
		</param>

		<param type="numeric" name="camera" default="1" label="camera" hint="Sets the camera">
			<min value="0" />
			<max value="4" />
		</param>

	</params>


</behavior>